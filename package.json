{
  "name": "@frak-labs/shopify-app",
  "private": true,
  "scripts": {
    "build": "remix vite:build",
    "config:link": "shopify app config link",
    "config:use": "shopify app config use",
    "db:generate": "sst dev drizzle-kit generate",
    "db:migrate": "sst dev drizzle-kit migrate",
    "db:studio": "sst dev drizzle-kit studio",
    "deploy": "sst deploy --stage dev",
    "deploy:prod": "sst deploy --stage production",
    "shopify:deploy": "shopify app deploy --config development",
    "shopify:deploy:prod": "shopify app deploy --config production",
    "dev": "bun sst dev",
    "shopify:dev": "shopify app dev --config development",
    "shopify:dev:prod": "shopify app dev --config production",
    "docker-start": "bun run setup && bun run start",
    "env": "shopify app env",
    "format": "biome check --write .",
    "format:check": "biome check .",
    "generate": "shopify app generate",
    "graphql-codegen": "graphql-codegen",
    "lint": "biome lint .",
    "shopify": "shopify",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc --noEmit",
    "update:deps": "npm-check-updates -ui -ws",
    "vite": "vite"
  },
  "type": "module",
  "engines": {
    "node": "^20.10 || >=21.0.0"
  },
  "dependencies": {
